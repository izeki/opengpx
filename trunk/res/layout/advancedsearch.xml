				android:layout_height="wrap_content" android:layout_marginLeft="10dp"></CheckBox>
			<ImageView android:layout_width="wrap_content" android:id="@+id/AdvSchImageViewVirtual"
				android:layout_centerVertical="true" android:layout_toRightOf="@+id/AdvSchTypeVirtual"
				android:layout_height="wrap_content" android:src="@drawable/virtual"></ImageView>
			<CheckBox android:id="@+id/AdvSchTypeWhereigo"
				android:layout_width="wrap_content" android:layout_centerVertical="true"
				android:layout_toRightOf="@+id/AdvSchImageViewVirtual"
				android:layout_height="wrap_content" android:layout_marginLeft="10dp"></CheckBox>
			<ImageView android:layout_width="wrap_content" android:id="@+id/AdvSchImageViewWhereigo"
				android:layout_centerVertical="true" android:layout_toRightOf="@+id/AdvSchTypeWhereigo"
				android:layout_height="wrap_content" android:src="@drawable/whereigo"></ImageView>
		</RelativeLayout>

		<RelativeLayout android:id="@+id/AdvSchRelativeLayout06"
			android:layout_width="wrap_content" android:layout_height="wrap_content">
			<CheckBox android:id="@+id/AdvSchTypeMegaevent"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:layout_centerVertical="true"></CheckBox>
			<ImageView android:layout_width="wrap_content" android:id="@+id/AdvSchImageViewMegaevent"
				android:layout_toRightOf="@+id/AdvSchTypeMegaevent"
				android:layout_height="wrap_content" android:layout_centerVertical="true"
				android:src="@drawable/megaevent"></ImageView>
			<CheckBox android:id="@+id/AdvSchTypeGpsame"
				android:layout_width="wrap_content" android:layout_toRightOf="@+id/AdvSchImageViewMegaevent"
				android:layout_height="wrap_content" android:layout_centerVertical="true"
				android:layout_marginLeft="10dp"></CheckBox>
			<ImageView android:layout_width="wrap_content" android:id="@+id/AdvSchImageViewGpsame"
				android:layout_toRightOf="@+id/AdvSchTypeGpsame"
				android:layout_height="wrap_content" android:layout_centerVertical="true"
				android:src="@drawable/gpsame"></ImageView>
			<CheckBox android:id="@+id/AdvSchTypeAll"
				android:layout_width="wrap_content" android:layout_centerVertical="true"
				android:layout_toRightOf="@+id/AdvSchImageViewGpsame"
