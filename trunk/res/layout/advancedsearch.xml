

<ScrollView android:id="@+id/AdvSchScrollView01"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<LinearLayout android:id="@+id/AdvSchLinearLayout01"
		android:layout_height="fill_parent" android:layout_width="fill_parent"
		android:orientation="vertical">
		<TextView android:layout_height="wrap_content"
			android:layout_width="fill_parent" android:gravity="center"
			android:textColor="#000000" android:background="#c8c8c8"
			android:textSize="12dp" android:text="Advanced Search"
			android:layout_marginBottom="2dp"></TextView>

		<LinearLayout android:id="@+id/AdvSchDiffTextLayout01"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:orientation="horizontal">
			<LinearLayout android:id="@+id/AdvSchDiffTextLayout02"
				android:layout_width="70dp" android:orientation="vertical"
				android:layout_height="fill_parent">
				<RelativeLayout android:layout_height="fill_parent"
					android:layout_width="fill_parent" android:id="@+id/AdvSchDiffTextLayout03">
					<TextView android:id="@+id/AdvSchDiffText"
						android:layout_width="wrap_content" android:layout_height="wrap_content"
						android:layout_centerVertical="true" android:text="Difficulty:"
						android:layout_alignParentLeft="true"></TextView>
				</RelativeLayout>
			</LinearLayout>
			<LinearLayout android:id="@+id/AdvSchDiffSpinnerLayout01"
				android:layout_height="wrap_content" android:orientation="vertical"
				android:layout_width="fill_parent">
				<RelativeLayout android:layout_height="wrap_content"
					android:layout_width="fill_parent" android:id="@+id/AdvSchDiffSpinnerLayout02">
					<Spinner android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:entries="@array/diff_terr_list"
						android:id="@+id/AdvSchDiffFromSpinner"
						android:layout_centerInParent="false"
						android:layout_centerVertical="true" android:layout_marginLeft="15dp"></Spinner>
					<TextView android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:layout_margin="5dp"
						android:text="To:" android:id="@+id/AdvSchDiffToText"
						android:layout_centerInParent="false" android:layout_toRightOf="@+id/AdvSchDiffFromSpinner"
						android:layout_centerVertical="true"></TextView>
					<Spinner android:layout_width="wrap_content"
						android:layout_centerVertical="true" android:layout_height="wrap_content"
						android:entries="@array/diff_terr_list" android:id="@+id/AdvSchDiffToSpinner"
						android:layout_alignParentRight="false" android:layout_toRightOf="@+id/AdvSchDiffToText"></Spinner>
				</RelativeLayout>
			</LinearLayout>
		</LinearLayout>

		<LinearLayout android:id="@+id/AdvSchTerrTextLayout01"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:orientation="horizontal">
			<LinearLayout android:id="@+id/AdvSchTerrTextLayout02"
				android:layout_width="70dp" android:orientation="vertical"
				android:layout_height="fill_parent">
				<RelativeLayout android:layout_height="fill_parent"
					android:layout_width="fill_parent" android:id="@+id/AdvSchTerrTextLayout03">
					<TextView android:id="@+id/AdvSchTerrText"
						android:layout_width="wrap_content" android:layout_height="wrap_content"
						android:layout_centerVertical="true" android:text="Terrain:"
						android:layout_alignParentLeft="true"></TextView>
				</RelativeLayout>
			</LinearLayout>
			<LinearLayout android:id="@+id/AdvSchTerrSpinnerLayout01"
				android:layout_height="wrap_content" android:orientation="vertical"
				android:layout_width="fill_parent">
				<RelativeLayout android:layout_height="wrap_content"
					android:layout_width="fill_parent" android:id="@+id/AdvSchTerrSpinnerLayout02">
					<Spinner android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:entries="@array/diff_terr_list"
						android:id="@+id/AdvSchTerrFromSpinner"
						android:layout_centerInParent="false"
						android:layout_centerVertical="true" android:layout_marginLeft="15dp"></Spinner>
					<TextView android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:layout_margin="5dp"
						android:text="To:" android:id="@+id/AdvSchTerrToText"
						android:layout_centerInParent="false" android:layout_toRightOf="@+id/AdvSchTerrFromSpinner"
						android:layout_centerVertical="true"></TextView>
					<Spinner android:layout_width="wrap_content"
						android:layout_centerVertical="true" android:layout_height="wrap_content"
						android:entries="@array/diff_terr_list" android:id="@+id/AdvSchTerrToSpinner"
						android:layout_alignParentRight="false" android:layout_toRightOf="@+id/AdvSchTerrToText"></Spinner>
				</RelativeLayout>
			</LinearLayout>
		</LinearLayout>

		<TextView android:layout_height="wrap_content"
			android:layout_width="fill_parent" android:gravity="center"
			android:textColor="#000000" android:background="#c8c8c8"
			android:textSize="12dp" android:text="Container Size"
			android:layout_marginBottom="2dp"></TextView>
			
		<LinearLayout android:id="@+id/AdvSchLinearLayout02"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:orientation="horizontal">
			<LinearLayout android:id="@+id/AdvSchLinearLayout02"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:orientation="vertical">
				<CheckBox android:id="@+id/AdvSchSizeLarge"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:text="Large"></CheckBox>
				<CheckBox android:id="@+id/AdvSchSizeRegular"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:text="Regular"></CheckBox>
				<CheckBox android:id="@+id/AdvSchSizeSmall"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:text="Small"></CheckBox>
			</LinearLayout>
			<LinearLayout android:id="@+id/AdvSchLinearLayout03"
				android:layout_width="100dp" android:layout_height="wrap_content"
				android:orientation="vertical">
				<CheckBox android:id="@+id/AdvSchSizeMicro"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:text="Micro" android:layout_marginLeft="5dp" android:layout_marginRight="5dp"></CheckBox>
				<CheckBox android:id="@+id/AdvSchSizeOther"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:text="Other" android:layout_marginLeft="5dp" android:layout_marginRight="5dp"></CheckBox>
				<CheckBox android:id="@+id/AdvSchSizeVirtual"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:text="Virtual" android:layout_marginLeft="5dp" android:layout_marginRight="5dp"></CheckBox>
			</LinearLayout>
			<LinearLayout android:id="@+id/AdvSchLinearLayout04"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:orientation="vertical">
				<CheckBox android:id="@+id/AdvSchSizeNotChosen"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:text="Not Chosen"></CheckBox>
				<CheckBox android:id="@+id/AdvSchSizeAll"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:text="All"></CheckBox>
				<CheckBox android:id="@+id/AdvSchSizeNone"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:text="None"></CheckBox>
			</LinearLayout>
		</LinearLayout>

		<TextView android:layout_height="wrap_content"
			android:layout_width="fill_parent" android:gravity="center"
			android:textColor="#000000" android:background="#c8c8c8"
			android:textSize="12dp" android:text="Cache Type"
			android:layout_marginBottom="2dp"></TextView>

		<RelativeLayout android:id="@+id/AdvSchRelativeLayout03"
			android:layout_width="wrap_content" android:layout_height="wrap_content">
			<CheckBox android:id="@+id/AdvSchTypeTraditional"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:layout_centerVertical="true"></CheckBox>
			<ImageView android:layout_width="wrap_content" android:id="@+id/AdvSchImageViewTraditional"
				android:layout_toRightOf="@+id/AdvSchTypeTraditional"
				android:layout_height="wrap_content" android:src="@drawable/traditional"
				android:layout_centerVertical="true"></ImageView>
			<CheckBox android:id="@+id/AdvSchTypeMulti"
				android:layout_width="wrap_content" android:layout_toRightOf="@+id/AdvSchImageViewTraditional"
				android:layout_height="wrap_content" android:layout_centerVertical="true"
				android:layout_marginLeft="10dp"></CheckBox>
			<ImageView android:layout_width="wrap_content" android:id="@+id/AdvSchImageViewMulti"
				android:layout_toRightOf="@+id/AdvSchTypeMulti"
				android:layout_height="wrap_content" android:src="@drawable/multi"
				android:layout_centerVertical="true"></ImageView>
			<CheckBox android:id="@+id/AdvSchTypeCito"
				android:layout_width="wrap_content" android:layout_centerVertical="true"
				android:layout_toRightOf="@+id/AdvSchImageViewMulti"
				android:layout_height="wrap_content" android:layout_marginLeft="10dp"></CheckBox>
			<ImageView android:layout_width="wrap_content" android:id="@+id/AdvSchImageViewCito"
