					android:layout_width="fill_parent" android:id="@+id/AdvSchActiveTextLayout03">
					<TextView android:id="@+id/AdvSchActiveText"
						android:layout_width="wrap_content" android:layout_height="wrap_content"
						android:layout_centerVertical="true" android:text="Is Active:"
						android:layout_alignParentLeft="true"></TextView>
				</RelativeLayout>
			</LinearLayout>
			<LinearLayout android:id="@+id/AdvSchActiveButtonLayout01"
				android:layout_height="wrap_content" android:orientation="vertical"
				android:layout_width="fill_parent">
