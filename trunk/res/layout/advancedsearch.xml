						<RadioButton android:text="Yes" android:id="@+id/AdvSchActiveYes"
							android:layout_width="wrap_content" android:layout_height="wrap_content"></RadioButton>
						<RadioButton android:text="No" android:id="@+id/AdvSchActiveNo"
							android:layout_width="wrap_content" android:layout_height="wrap_content"
							android:layout_marginLeft="5dp" android:layout_marginRight="5dp"></RadioButton>
						<RadioButton android:text="Either" android:id="@+id/AdvSchActiveEither"
							android:layout_width="wrap_content" android:layout_height="wrap_content"
							android:checked="true"></RadioButton>
					</RadioGroup>
				</RelativeLayout>
			</LinearLayout>
		</LinearLayout>

